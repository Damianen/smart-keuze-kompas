# Database Configuration
DB_NAME="your_database_name"
STRING_CONNECTION="mongodb+srv://user:password@host/?retryWrites=true&w=majority"

# Environment
PRODUCTION=false

# FastAPI Integration
API_KEY="your_fastapi_api_key"
API_KEY_NAME="Token"
FAST_API_URL="https://your-fastapi-url.com/recommend"

# Authentication
JWT_SECRET="your_jwt_secret_min_32_chars"

# Logging (Seq)
SEQ_API_KEY="your_seq_api_key"
SEQ_Base_URL="https://your-seq-url.com"

# Rate Limiting
THROTTLE_TTL=60
THROTTLE_LIMIT=100

# Server
PORT=3000
